    <h2 id="forms.heading">Forms</h2>

    <form id="form" class="form" title="form" xml:lang="en" dir="ltr" action="uri" method="post" enctype="application/x-www-form-urlencoded" accept-charset="UTF-8, UTF-16, UTF-16BE, UTF-16LE" lang="en">
      <fieldset id="fieldset" class="fieldset" title="Fieldset" xml:lang="en" dir="ltr" lang="en">
      <legend id="legend" class="legend" title="Legend" xml:lang="en" dir="ltr" accesskey="L" lang="en">Input fields</legend>

      <fieldset>
      <legend>Text input</legend>

      <p>
      <label id="label" class="label" title="Label" xml:lang="en" dir="ltr" for="input.text" lang="en">Text input field:</label>
      <input id="input.text" class="input" title="Text input" xml:lang="en" dir="ltr" type="text" name="text" value="default text" size="40" tabindex="8" accesskey="T" lang="en" />
      </p>

      <p>
      <label for="input.text.disabled">Disabled text input field:</label>
      <input id="input.text.disabled" class="input" title="Disabled text input" xml:lang="en" dir="ltr" type="text" name="text" value="default text" disabled="disabled" size="40" lang="en" />
      </p>

      <p>
      <label for="input.text.readonly">Readonly text input field:</label>
      <input id="input.text.readonly" class="input" title="Readonly text input" xml:lang="en" dir="ltr" type="text" name="text" value="readonly text" readonly="readonly" size="15" lang="en" />
      </p>

      </fieldset>

      <fieldset>
      <legend>Password input</legend>

